{
  "hosting": {
    "public": ".",
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/articles",
        "destination": "/articles.html"
      },
      {
        "source": "/articles/**",
        "destination": "/article.html"
      }
    ],
    "redirects": [
      {
        "source": "/board",
        "destination": "/articles",
        "type": 301
      },
      {
        "source": "/board.html",
        "destination": "/articles",
        "type": 301
      }
    ]
  }
}
